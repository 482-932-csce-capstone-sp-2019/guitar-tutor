GuitarScreen:
    # Name of the screen
	name: "PlayingTab"
	
	# fullscreen helps with getting the contents formatted correctly
	fullscreen: True
    OnScreenTab:
        pos: self.pos


<OnScreenTab>:
    canvas:
        Color:
            rgba: 1,0,0,1
        Rectangle:
            pos: self.pos
            size: self.size
    Label:
        text: root.stringEHigh