GuitarScreen:
    # Name of the screen
	name: "PlayingTab"
	
	# fullscreen helps with getting the contents formatted correctly
	fullscreen: True
	
	ActionBar:
		background_color: 0, 0, 0 ,0
		ActionView:
			ActionPrevious:
				app_icon: 'data/icons/home.png'
				on_release: app.go_screen(0), app.stop_tuner(), app.stopTab()
	
	
	
    OnScreenTab:
        pos: self.pos


<OnScreenTab>:
    canvas:
        Color:
            rgba: 1,0,0,1
        Rectangle:
            pos: self.pos
            size: self.size
    Label:
        text: root.stringEHigh